{
  "name": "orchestrator-api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config --respawn --transpile-only --poll --interval 300 src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "lint": "echo lint",
    "test": "cross-env DOTENV_CONFIG_PATH=./.env.test node --test -r ts-node/register -r dotenv/config --test-reporter=spec --test-timeout=30000 \"src/__tests__/**/*.test.ts\"",
    "test:watch": "cross-env DOTENV_CONFIG_PATH=./.env.test node --test -r ts-node/register -r dotenv/config --watch --test-reporter=spec --test-timeout=30000 \"src/__tests__/**/*.test.ts\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "mariadb": "^3.4.5",
    "mysql2": "^3.14.3",
    "pg": "^8.16.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.11",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "cross-env": "^10.0.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@10.14.0"
}
